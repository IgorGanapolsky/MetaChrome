{
  "name": "metachrome",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./scripts/start-prefer-device.js",
    "start:rozenite": "WITH_ROZENITE=true node ./scripts/start-prefer-device.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:fix": "jest --no-coverage --passWithNoTests",
    "prepare": "husky install || true",
    "reset-project": "node ./scripts/reset-project.js",
    "expo:go": "node scripts/start-expo-go.js",
    "build:production": "bash scripts/build-production.sh",
    "uat": "bash scripts/run-uat.sh",
    "add:lesson": "node scripts/add-lesson.js",
    "ios:auto": "node scripts/ios-auto.js",
    "android:auto": "node scripts/android-auto.js",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "validate:config": "node scripts/validate-expo-config.js",
    "preflight": "node scripts/preflight-check.js",
    "preflight:full": "node scripts/preflight-check.js --full",
    "test:smoke": "jest --testPathPattern=smoke --passWithNoTests",
    "test:all": "yarn preflight && yarn test && yarn test:smoke",
    "thumbs": "bash scripts/thumbs.sh",
    "memory:add": "node scripts/memory-add.js",
    "memory:list": "node scripts/memory-list.js",
    "memory:forget": "node scripts/memory-forget.js",
    "memory:reflect": "node scripts/memory-reflect.js",
    "docs:sync": "node scripts/docs-sync.js",
    "rag:ingest": "npx -y tsx scripts/run-ingest.ts",
    "rag:build": "echo 'Deprecated: use rag:ingest --dry-run' && npx -y tsx scripts/run-ingest.ts --dry-run",
    "rag:publish": "echo 'Deprecated: use rag:ingest' && npx -y tsx scripts/run-ingest.ts",
    "graph:build": "npx -y tsx --tsconfig tsconfig.scripts.json -r ./scripts/setup-mocks.ts scripts/graph-build.ts",
    "test:graph": "npx -y tsx --tsconfig tsconfig.scripts.json -r ./scripts/setup-mocks.ts scripts/test-graph.ts",
    "verify:graph": "npx -y tsx --tsconfig tsconfig.scripts.json -r ./scripts/setup-mocks.ts scripts/debug-graph.ts",
    "kernel:bench": "node scripts/kernel-bench.js",
    "kernel:select": "node scripts/kernel-select.js",
    "kernel:gen": "python3 scripts/kernel-generate.py",
    "kernel:loop": "yarn kernel:gen && yarn kernel:bench && yarn kernel:select && yarn rag:build",
    "agent:autopilot": "node scripts/agent-autopilot.js",
    "agent:dev": "node scripts/agent-dev.js",
    "langsmith:sync": "python3 scripts/langsmith-sync.py",
    "langsmith:check": "LANGSMITH_FAIL_IF_EMPTY=true python3 scripts/langsmith-sync.py",
    "eval:monitorability": "node scripts/eval-monitorability.js"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.3",
    "@google-labs/agent-kit": "^0.18.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "^2.2.0",
    "expo": "~54.0.30",
    "expo-application": "^7.0.8",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "^6.0.20",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "^15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-speech": "^14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-native": "0.83.1",
    "react-native-ble-plx": "^3.5.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.19.0",
    "react-native-siri-shortcut": "^3.2.4",
    "react-native-web": "~0.21.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "0.7.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@rozenite/metro": "^1.1.0",
    "@rozenite/react-navigation-plugin": "^1.1.0",
    "@sentry/react-native": "^7.8.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/react": "~19.2.7",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "module-alias": "^2.2.3",
    "neo4j-driver": "^6.0.1",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.2.3",
    "reactotron-react-native": "^5.1.18",
    "sentry-expo": "^7.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
